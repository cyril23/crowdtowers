{
  "name": "alien-invasion-tower-defense",
  "version": "1.0.0",
  "type": "module",
  "description": "Multiplayer cooperative tower defense game",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "node scripts/build.cjs",
